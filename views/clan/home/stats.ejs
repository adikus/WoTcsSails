<div class="stats-switch js-switch">
    <%- link_to('Average', '#', {class: 'active'}) %> / <%- link_to('Overall', '#') %>
</div>

<div class="progress progress-big">
    <div class="progress-bar progress-bar-danger" role="progressbar" style="width: 65%;">
        Battles
    </div>
    <div class="progress-value">25000</div>
</div>
<div class="progress progress-big">
    <div class="progress-bar progress-bar-success" role="progressbar" style="width: 82%;">
        Winrate
    </div>
    <div class="progress-value">55%</div>
</div>
<div class="progress progress-big">
    <div class="progress-bar" role="progressbar" style="width: 93%;">
        Damage
    </div>
    <div class="progress-value">1350</div>
</div>
<div class="progress progress-big">
    <div class="progress-bar progress-bar-success" role="progressbar" style="width: 82%;">
        WN7
    </div>
    <div class="progress-value">1550</div>
</div>
<div class="progress progress-big">
    <div class="progress-bar progress-bar-warning" role="progressbar" style="width: 75%;">
        Efficiency
    </div>
    <div class="progress-value">1490</div>
</div>